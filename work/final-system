./configure \
    --prefix=/usr \
    --enable-no-install-program=kill,uptime \
    --enable-install-program=hostname
make
# make -k NON_ROOT_USERNAME=nobody check-root >> $TEST_LOG 2>&1 || true
# echo "dummy:x:1000:nobody" >> /etc/group
# chown -Rv nobody . >> $TEST_LOG 2>&1
# su nobody -s /bin/bash \
    -c "PATH=$PATH make -k RUN_EXPENSIVE_TESTS=yes -k check || true" >> $TEST_LOG 2>&1 || true
# sed -i '/dummy/d' /etc/group >> $TEST_LOG 2>&1
make -j1 install
mv -v /usr/bin/{cat,chgrp,chmod,chown,cp,date} /bin
mv -v /usr/bin/{dd,df,echo,false,hostname,ln,ls,mkdir,mknod} /bin
mv -v /usr/bin/{mv,pwd,rm,rmdir,stty,true,uname} /bin
