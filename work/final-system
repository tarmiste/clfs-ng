set +h
set -e
cd $PKGDIR
export BUILD_ZLIB=False
export BUILD_BZIP2=0
# echo "127.0.0.1 localhost $(hostname)" > /etc/hosts >> $TEST_LOG 2>&1
./configure.gnu \
    --prefix=/usr \
    -Dvendorprefix=/usr \
    -Dman1dir=/usr/share/man/man1 \
    -Dman3dir=/usr/share/man/man3 \
    -Dpager="/bin/less -isR" \
    -Dcc="gcc ${BUILD32}" \
    -Dusethreads \
    -Duseshrplib
make
# make -k test >> $TEST_LOG 2>&1 || true
make -j1 install
unset BUILD_ZLIB BUILD_BZIP2
mv -v /usr/bin/perl{,-32}
mv -v /usr/bin/perl5.26.0{,-32}
echo -e "\n\nTotalseconds: $SECONDS\n"
exit=========================================== perl-64bit
#!/bin/bash
set +h
set -e
cd $PKGDIR
sed -i -e '/^BUILD_ZLIB/s/True/False/' \
       -e '/^INCLUDE/s,\./zlib-src,/usr/include,' \
       -e '/^LIB/s,\./zlib-src,/usr/lib64,' \
       cpan/Compress-Raw-Zlib/config.in
patch -Np1 -i ../perl-5.26.0-Configure_multilib-1.patch
echo 'installstyle="lib64/perl5"' >> hints/linux.sh
./configure.gnu \
    --prefix=/usr \
    -Dvendorprefix=/usr \
    -Dman1dir=/usr/share/man/man1 \
    -Dman3dir=/usr/share/man/man3 \
    -Dpager="/bin/less -isR" \
    -Dlibpth="/usr/local/lib64 /lib64 /usr/lib64" \
    -Dcc="gcc ${BUILD64}" \
    -Dusethreads \
    -Duseshrplib
make
# make -k test >> $TEST_LOG 2>&1 || true
make -j1 install
unset BUILD_ZLIB BUILD_BZIP2
mv -v /usr/bin/perl{,-64}
mv -v /usr/bin/perl5.26.0{,-64}
ln -sv multiarch_wrapper /usr/bin/perl
ln -sv multiarch_wrapper /usr/bin/perl5.26.0
echo -e "\n\nTotalseconds: $SECONDS\n"
exit=========================================== readline
#!/bin/bash
set +h
set -e
cd $PKGDIR
patch -Np1 -i ../readline-7.0-branch_update-1.patch
sed -i '/MV.*old/d' Makefile.in
sed -i '/{OLDSUFF}/c:' support/shlib-install
CC="gcc ${BUILD32}" CXX="g++ ${BUILD32}" \
./configure \
    --prefix=/usr \
    --libdir=/lib \
    --docdir=/usr/share/doc/readline-7.0
make SHLIB_LIBS=-lncurses
make -j1 SHLIB_LIBS=-lncurses htmldir=/usr/share/doc/readline-7.0 install
mv -v /lib/lib{readline,history}.a /usr/lib
ln -svf ../../lib/$(readlink /lib/libreadline.so) /usr/lib/libreadline.so
ln -svf ../../lib/$(readlink /lib/libhistory.so) /usr/lib/libhistory.so
rm -v /lib/lib{readline,history}.so
echo -e "\n\nTotalseconds: $SECONDS\n"
exit=========================================== readline-64bit
