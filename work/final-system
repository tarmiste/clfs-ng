#!/bin/bash
set +h
set -e
cd $PKGDIR
cat > bc/fix-libmath_h << "EOF"
#! /bin/bash
sed -e '1   s/^/{"/' \
    -e     's/$/",/' \
    -e '2,$ s/^/"/'  \
    -e   '$ d'       \
    -i libmath.h

sed -e '$ s/$/0}/' \
-i libmath.h
EOF
CC="gcc ${BUILD64}" \
./configure \
    --prefix=/usr \
    --mandir=/usr/share/man \
    --infodir=/usr/share/info
make
# echo "quit" | ./bc/bc -l Test/checklib.b >> $TEST_LOG 2>&1
make -j1 install
echo -e "\n\nTotalseconds: $SECONDS\n"
exit=========================================== diffutils
