    <xi:include xmlns:xi="http://www.w3.org/2001/XInclude"
    href="../common/python.xml"
    xpointer="xpointer(//*[@os='a'])"/>

<screen os="b"><userinput>CC="gcc ${BUILDN32}" ./configure \
    ADJTIME_PATH=/var/lib/hwclcock/adjtime \
    --libdir=/lib32 \
    --enable-write \
    --disable-chfn-chsh \
    --disable-login \
    --disable-nologin \
    --disable-su \
    --disable-setpriv \
    --disable-runuser \
    --docdir=/usr/share/doc/python-&python-version;</userinput></screen>

